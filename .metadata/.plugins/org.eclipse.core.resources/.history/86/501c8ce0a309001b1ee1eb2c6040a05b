package School;

import java.util.Scanner;

public class StudentInfo {

	public static void main(String[] args) {
		Students st1= new Students("Amanda",1,'F');
		Students st2= new Students("Todd",1,'M');
		Students st3= new Students("Harry", 2,'M');
		Students st4= new Students("Ashley",2,'F');
		
		System.out.println("Enter Two Students");
		Scanner sc = new Scanner(System.in);
		String stName1 =sc.next();
		String stName2 =sc.next();
		sc.close();
		
		if(stName1==stName2) {System.out.println("They are the same gender");
		
		}else {System.out.println("They are not the same gender");
		
		}

	}

}

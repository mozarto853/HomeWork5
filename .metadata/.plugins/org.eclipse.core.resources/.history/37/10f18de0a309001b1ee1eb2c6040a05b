package School;

public class Students {
	
	private String stName;
	private int stClass;
	private char stGender;
	
	public String getStName() {
		return stName;
	}
	public void setStName(String stName) {
		this.stName = stName;
	}
	public int getStClass() {
		return stClass;
	}
	public void setStClass(int stClass) {
		this.stClass = stClass;
	}
	public char getStGender() {
		return stGender;
	}
	public void setStGender(char stGender) {
		this.stGender = stGender;
	}
	
	public Students(String stName, int stClass, char stGender) {
		super();
		this.stName = stName;
		this.stClass = stClass;
		this.stGender = stGender;
	}
	
	
	

}

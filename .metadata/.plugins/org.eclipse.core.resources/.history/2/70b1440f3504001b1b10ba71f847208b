package variables;


public class TestCalculator {

	public static void main(String[] args) {
		Calculator cal = new Calculator();
		int result = cal.addition(75, 83);
		System.out.println("The addition of the two numbers equals " + result);;;;;
		
		int result2 = cal.subtraction(200, 153);
		System.out.println("The subtraction of the two numbers equal " + result2);
		

	}

}
